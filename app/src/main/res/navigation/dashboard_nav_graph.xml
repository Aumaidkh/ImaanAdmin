<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dashboard_nav_graph"
    app:startDestination="@id/fragmentDashboard">

    <fragment
        android:id="@+id/fragmentDashboard"
        android:name="com.imaan.imaanadmin.presentation.dashboard.FragmentDashboard"
        android:label="FragmentDashboard" >
        <action
            android:id="@+id/action_fragmentDashboard_to_orderDetailsFragment"
            app:destination="@id/orderDetailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.imaan.imaanadmin.presentation.orders.fragments.details.OrderDetailsFragment"
        android:label="OrderDetailsFragment" >
        <argument
            android:name="ORDER_ID"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="null" />
    </fragment>
</navigation>